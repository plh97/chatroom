  "scripts": {
    "eslint": "eslint ./src --color --fix",
    "w": "webpack --color --watch",
    "b": "cross-env NODE_ENV=development PLATFORM=web webpack --color",
    "p": "cross-env NODE_ENV=production PLATFORM=web webpack -p --color",
    "pm2": "cross-env NODE_ENV=production pm2 start ./src/server/index.js --watch",
    "nodemon": "cross-env NODE_ENV=development nodemon --inspect ./src/server/index.js",
    "restart": "cross-env NODE_ENV=production pm2 restart ./src/server/index.js",
    "t": "webpack-dev-server --open --color",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "test": "cross-env NODE_ENV=test && mocha test/*",
    "lint": "./node_modules/.bin/eslint ./ --ext js,jsx --ignore-pattern .eslintignore --cache --fix"
  },
